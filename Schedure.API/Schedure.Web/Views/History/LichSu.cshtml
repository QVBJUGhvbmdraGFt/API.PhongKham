@model List<SchedureDTO.HistoryKhamBenhDTO>

@{
    ViewBag.Title = "LichSu";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>
            MÃ ĐĂNG KÍ
        </th>
        <th>
            THỜI GIAN ĐĂNG KÍ
        </th>
        <th>
            BÁC SĨ KHÁM
        </th>
        <th>
            THỜI GIAN KHÁM
        </th>
        <th>
            TRIỆU CHỨNG LÂM SÀN
        </th>
        <th>
            CHẨN ĐOÁN SƠ BỘ
        </th>
        <th>
            CÁCH GIẢI QUYẾT
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IDRegister)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Register_CreateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TrieuChungLS)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChuanDoanSoBo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CachGiaiQuyet)
            </td>
            <td>
                <a href="@Url.Action("Detail",new { IDHistory = item.IDHistory})">Kết quả</a>
            </td>
        </tr>
    }

</table>
